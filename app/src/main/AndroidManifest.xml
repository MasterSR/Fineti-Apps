<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fineti.myfineti.fineti" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.SEND_SMS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >

        <activity
            android:name="com.fineti.myfineti.fineti.SplashScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.fineti.myfineti.fineti.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MainActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.fineti.myfineti.fineti.Login" >
        </activity>
        <activity android:name="com.fineti.myfineti.fineti.Register" >
        </activity>
        <activity android:name="com.fineti.myfineti.fineti.Survey" >
        </activity>
        <activity android:name="com.fineti.myfineti.fineti.Report" >
        </activity>
        <activity android:name="com.fineti.myfineti.fineti.Agenda" >
        </activity>
        <activity android:name="com.fineti.myfineti.fineti.mWebView" >
        </activity>
        <activity android:name="com.fineti.myfineti.fineti.Menual_Survey" >
        </activity>
        <activity android:name="com.fineti.myfineti.fineti.MProfile" >
        </activity>
        <activity android:name="com.fineti.myfineti.fineti.text_massage" >
        </activity>
        <activity android:name="com.fineti.myfineti.fineti.Zonal_RVew" >
        </activity>
    </application>

</manifest>
